{"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"CheckpointMode"}],"languages":["swift"],"platforms":["macOS"]}]}],"schemaVersion":{"major":0,"minor":2,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/glider\/sqlitedb\/checkpointmode"],"traits":[{"interfaceLanguage":"swift"}]}],"relationshipsSections":[{"identifiers":["doc:\/\/Glider\/SQ","doc:\/\/Glider\/SH"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"identifier":{"url":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"See "},{"type":"reference","isActive":true,"identifier":"https:\/\/sqlite.org\/c3ref\/wal_checkpoint_v2.html"}],"kind":"symbol","metadata":{"navigatorTitle":[{"kind":"identifier","text":"CheckpointMode"}],"role":"symbol","title":"SQLiteDb.CheckpointMode","roleHeading":"Enumeration","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"CheckpointMode"}],"symbolKind":"enum","externalID":"s:6Glider8SQLiteDbC14CheckpointModeO","extendedModule":"Glider","modules":[{"name":"Glider"}]},"hierarchy":{"paths":[["doc:\/\/Glider\/documentation\/Glider","doc:\/\/Glider\/documentation\/Glider\/SQLiteDb"]]},"topicSections":[{"title":"Enumeration Cases","identifiers":["doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/full","doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/passive","doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/restart","doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/truncate"]},{"title":"Default Implementations","identifiers":["doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/Equatable-Implementations"],"generated":true}],"references":{"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode/Equatable-Implementations":{"role":"collectionGroup","title":"Equatable Implementations","abstract":[],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/Equatable-Implementations","kind":"article","type":"topic","url":"\/documentation\/glider\/sqlitedb\/checkpointmode\/equatable-implementations"},"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode/truncate":{"role":"symbol","title":"SQLiteDb.CheckpointMode.truncate","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"truncate"}],"abstract":[{"type":"codeVoice","code":"SQLITE_CHECKPOINT_TRUNCATE"},{"type":"text","text":" "},{"type":"text","text":"This mode works the same way as SQLITE_CHECKPOINT_FULL with the addition that after checkpointing"},{"type":"text","text":" "},{"type":"text","text":"the log file it blocks (calls the busy-handler callback) until all readers are reading from the"},{"type":"text","text":" "},{"type":"text","text":"database file only. This ensures that the next writer will restart the log file from the beginning."},{"type":"text","text":" "},{"type":"text","text":"Like SQLITE_CHECKPOINT_FULL, this mode blocks new database writer attempts while it is pending,"},{"type":"text","text":" "},{"type":"text","text":"but does not impede readers."}],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/truncate","kind":"symbol","type":"topic","url":"\/documentation\/glider\/sqlitedb\/checkpointmode\/truncate"},"https://sqlite.org/c3ref/wal_checkpoint_v2.html":{"title":"Checkpoint a database","titleInlineContent":[{"type":"text","text":"Checkpoint a database"}],"type":"link","identifier":"https:\/\/sqlite.org\/c3ref\/wal_checkpoint_v2.html","url":"https:\/\/sqlite.org\/c3ref\/wal_checkpoint_v2.html"},"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode/restart":{"role":"symbol","title":"SQLiteDb.CheckpointMode.restart","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"restart"}],"abstract":[{"type":"codeVoice","code":"SQLITE_CHECKPOINT_RESTART"}],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/restart","kind":"symbol","type":"topic","url":"\/documentation\/glider\/sqlitedb\/checkpointmode\/restart"},"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode":{"role":"symbol","title":"SQLiteDb.CheckpointMode","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"CheckpointMode"}],"abstract":[{"type":"text","text":"See "},{"type":"reference","isActive":true,"identifier":"https:\/\/sqlite.org\/c3ref\/wal_checkpoint_v2.html"}],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CheckpointMode"}],"url":"\/documentation\/glider\/sqlitedb\/checkpointmode"},"doc://Glider/documentation/Glider":{"role":"collection","title":"Glider","abstract":[],"identifier":"doc:\/\/Glider\/documentation\/Glider","kind":"symbol","type":"topic","url":"\/documentation\/glider"},"doc://Glider/SQ":{"type":"unresolvable","title":"Swift.Equatable","identifier":"doc:\/\/Glider\/SQ"},"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode/passive":{"role":"symbol","title":"SQLiteDb.CheckpointMode.passive","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"passive"}],"abstract":[{"type":"codeVoice","code":"SQLITE_CHECKPOINT_PASSIVE"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Checkpoint as many frames as possible without waiting for any database readers or writers to finish,"},{"type":"text","text":" "},{"type":"text","text":"then sync the database file if all frames in the log were checkpointed."},{"type":"text","text":" "},{"type":"text","text":"The busy-handler callback is never invoked in the SQLITE_CHECKPOINT_PASSIVE mode."},{"type":"text","text":" "},{"type":"text","text":"On the other hand, passive mode might leave the checkpoint unfinished if there are concurrent readers or writers."}],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/passive","kind":"symbol","type":"topic","url":"\/documentation\/glider\/sqlitedb\/checkpointmode\/passive"},"doc://Glider/documentation/Glider/SQLiteDb/CheckpointMode/full":{"role":"symbol","title":"SQLiteDb.CheckpointMode.full","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"full"}],"abstract":[{"type":"codeVoice","code":"SQLITE_CHECKPOINT_FULL"},{"type":"text","text":" "},{"type":"text","text":"This mode blocks (it invokes the busy-handler callback) until there is no database writer and all readers"},{"type":"text","text":" "},{"type":"text","text":"are reading from the most recent database snapshot."},{"type":"text","text":" "},{"type":"text","text":"It then checkpoints all frames in the log file and syncs the database file."},{"type":"text","text":" "},{"type":"text","text":"This mode blocks new database writers while it is pending, but new database readers are allowed to continue unimpeded."}],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb\/CheckpointMode\/full","kind":"symbol","type":"topic","url":"\/documentation\/glider\/sqlitedb\/checkpointmode\/full"},"doc://Glider/SH":{"type":"unresolvable","title":"Swift.Hashable","identifier":"doc:\/\/Glider\/SH"},"doc://Glider/documentation/Glider/SQLiteDb":{"role":"symbol","title":"SQLiteDb","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"SQLiteDb"}],"abstract":[],"identifier":"doc:\/\/Glider\/documentation\/Glider\/SQLiteDb","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SQLiteDb"}],"url":"\/documentation\/glider\/sqlitedb"}}}